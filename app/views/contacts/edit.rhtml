<h1>Edit Contact</h1>
<p>Contacts are the people who will receive and pay invoices. Each customer has one contact.</p>
<%= error_messages_for :contact %>
<% form_for(:contact, :url => contact_path(@contact), :html => { :method => :put }) do |f| %>
	<%= render(:partial => 'form', :object => @contact, 
			:locals => {
				:button_label => 'Save Changes',
				:f => f
			 }) %>
	<%= update_button %>
<% end %>
